{
  "name": "@ember-gettext/ember-l10n",
  "version": "0.4.0",
  "private": false,
  "description": "Handle translations in your Ember app with gettext.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/fabscale/ember-gettext/packages/ember-l10n",
  "license": "MIT",
  "author": "Francesco Novy <francesco@fabscale.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "run-extract": "ember gettext:extract -i tests/dummy",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-one": "ember try:one",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.2",
    "broccoli-plugin": "^4.0.7",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-fetch": "^8.1.0"
  },
  "devDependencies": {
    "@ember-gettext/gettext-parser": "0.4.0",
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.2.8",
    "@embroider/test-setup": "~0.42.3",
    "broccoli-asset-rev": "~3.0.0",
    "ember-cli": "~3.27.0",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-sri": "~2.1.1",
    "ember-cli-terser": "~4.0.2",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-maybe-import-regenerator": "~0.1.6",
    "ember-page-title": "~6.2.2",
    "ember-qunit": "~5.1.4",
    "ember-resolver": "~8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~3.5.0",
    "ember-try": "~1.4.0",
    "eslint-plugin-ember": "~10.5.1",
    "eslint-plugin-qunit": "^6.1.1",
    "loader.js": "~4.7.0",
    "npm-run-all": "~4.1.5",
    "qunit": "~2.16.0",
    "qunit-dom": "~1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "broccoli-asset-rev"
    ]
  }
}
